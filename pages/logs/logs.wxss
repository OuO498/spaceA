.indexContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  background:white;
  height: 100%;
}

.avatarUrl {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  margin: 100rpx 0 10rpx 0;
}
text{
  font-size: 32rpx;
  margin: 0;
}
button {
  width: 200rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 25rpx 0;
  font-size: 24rpx;
  line-height: 100rpx;
  text-align: center;
  background:white;
}

.userName {
  font-size: 32rpx;
  margin: 100rpx 0;
}

.goUrl{
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border: 1rpx solid #333;
  border-radius: 10rpx;
}